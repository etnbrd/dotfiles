[include]
        path = .gitconfig-flynn
[include]
        path = .gitconfig-user
[core]
        excludesfile = ~/.gitignore
        editor = vim
[hub]
        protocol = https        
[apply]
        whitespace = nowarn
[mergetool]
        keepBackup = false
[difftool]
        prompt = false
[help]
        autocorrect = 1
[push]
        default = simple
[color]
        diff = auto
        status = auto
        branch = auto
        ui = true
[alias]
        co = checkout